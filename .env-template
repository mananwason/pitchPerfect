# LLM provider: "bedrock" (default), "openai", or "gemini"
LLM_PROVIDER=bedrock

# AWS Bedrock (default) â€” uses your AWS credentials (~/.aws/credentials or env vars)
AWS_REGION=us-east-1
AWS_BEDROCK_MODEL_ID=anthropic.claude-opus-4-6-v1

# OpenAI (optional, only if LLM_PROVIDER=openai)
OPENAI_API_KEY=your-openai-api-key

# Google Gemini (optional, only if LLM_PROVIDER=gemini)
GOOGLE_API_KEY=your-google-api-key

# Device platform: "auto" (detect), "android", or "ios"
DEVICE_PLATFORM=auto

# Android: ADB host IP (default: 127.0.0.1 for local USB)
DEVICE_IP=127.0.0.1

# iOS: WebDriverAgent URL (start with: tidevice wdaproxy --port 8100)
WDA_URL=http://localhost:8100

# Number of profiles to process per run
NUM_PROFILES=10
